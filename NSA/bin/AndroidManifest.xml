<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nsa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoggedIn"
            android:label="@string/title_activity_logged_in" >
        </activity>
        <activity
            android:name=".Intrusion"
            android:label="@string/title_activity_intrusion" >
        </activity>
        <activity
            android:name=".AddAgent"
            android:label="@string/title_activity_add_agent" >
        </activity>
        <activity
            android:name=".SearchAgent"
            android:label="@string/title_activity_search_agent" >
        </activity>
    </application>

</manifest>
